<div class="panel-heading">
  <div class="panel-title">検索</div>
</div>
<div class="panel-body">
  <%= search_form_for @q do |f| %>
    <div class="row">
      <div class="form-group form-setting">
        <%= f.label :nickname_cont, "ニックネーム" %>
        <%= f.text_field :nickname_cont, class: "form-control" %>
      </div>

      <div class="form-group form-setting">
        <%= f.label :profile_cont, "プロフィール" %>
        <%= f.search_field :profile_cont, class: "form-control" %>
      </div>

    </div>
    <%= f.submit '検索', class: "btn btn-primary" %>
    <%= link_to 'クリア', url_for, class: "btn btn-default" %>
  <% end %>
</div>
